{
	"name": "@sixa/wp-block-container",
	"version": "0.2.0",
	"description": "Container block for WordPress editor.",
	"keywords": [
		"section",
		"group",
		"wrapper"
	],
	"homepage": "https://github.com/sixach/sixa-wp-block-container#readme",
	"bugs": {
		"url": "https://github.com/sixach/sixa-wp-block-container/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sixach/sixa-wp-block-container.git"
	},
	"license": "GPL-3.0-or-later",
	"author": "sixa AG",
	"main": "build/index.js",
	"scripts": {
		"dev": "wp-scripts start",
		"build": "wp-scripts build",
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style",
		"lint:css-fix": "wp-scripts lint-style --fix",
		"lint:js": "wp-scripts lint-js",
		"lint:js-fix": "wp-scripts lint-js --fix",
		"lint:pkg-json": "wp-scripts lint-pkg-json",
		"module-dev": "wp-scripts start --config submodule-webpack.config.js",
		"module-build": "wp-scripts build --config submodule-webpack.config.js",
		"packages-update": "wp-scripts packages-update"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.css": [
			"npm run lint:css"
		],
		"*.{js,ts,tsx}": [
			"npm run lint:js",
			"npm run format:js"
		]
	},
	"dependencies": {
		"@sixach/wp-block-components": "^0.1.1",
		"@wordpress/blob": "^3.0.0",
		"@wordpress/block-editor": "^6.0.0",
		"@wordpress/blocks": "^9.0.0",
		"@wordpress/components": "^14.0.0",
		"@wordpress/compose": "^4.0.0",
		"@wordpress/data": "^5.0.0",
		"@wordpress/element": "^3.0.0",
		"@wordpress/hooks": "^3.0.0",
		"@wordpress/i18n": "^4.0.0"
	},
	"devDependencies": {
		"@sixa/wp-block-utils": "^0.10.0",
		"@wordpress/scripts": "^16.0.0",
		"autoprefixer": "^9.8.6",
		"classnames": "^2.3.1",
		"husky": "^6.0.0",
		"lint-staged": "^11.0.0",
		"lodash": "^4.17.21",
		"postcss-calc": "^7.0.5",
		"postcss-each": "^0.10.0",
		"postcss-flexbox": "^1.0.3",
		"postcss-for": "^2.1.1",
		"postcss-import": "^12.0.1",
		"postcss-important-startstop": "^2.0.0",
		"postcss-loader": "^3.0.0",
		"postcss-nested": "^4.2.1",
		"postcss-nested-ancestors": "^2.0.0",
		"postcss-position": "^1.1.0",
		"postcss-selector-replace": "^1.0.2",
		"postcss-size": "^3.0.0"
	}
}
