{
	"name": "@sixach/wp-block-container",
	"version": "0.2.0",
	"description": "Container block for WordPress editor.",
	"keywords": [
		"section",
		"group",
		"wrapper"
	],
	"homepage": "https://github.com/sixach/sixa-wp-block-container#readme",
	"bugs": {
		"url": "https://github.com/sixach/sixa-wp-block-container/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sixach/sixa-wp-block-container.git"
	},
	"license": "GPL-3.0-or-later",
	"author": "sixa AG",
	"main": "build/index.js",
	"scripts": {
		"dev": "wp-scripts start",
		"build": "wp-scripts build",
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style",
		"lint:css-fix": "wp-scripts lint-style --fix",
		"lint:js": "wp-scripts lint-js",
		"lint:js-fix": "wp-scripts lint-js --fix",
		"lint:pkg-json": "wp-scripts lint-pkg-json",
		"lint:php": "composer lint:php",
		"lint:wpcs": "composer lint:wpcs",
		"lint:wpcs-fix": "composer lint:wpcbf",
		"packages-update": "wp-scripts packages-update"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.css": [
			"npm run lint:css"
		],
		"*.{js,ts,tsx}": [
			"npm run lint:js",
			"npm run format:js"
		],
		"*.php": [
			"npm run lint:wpcs"
		]
	},
	"dependencies": {
		"@sixach/wp-block-components": "^1.4.0",
		"@wordpress/blob": "^3.1.1",
		"@wordpress/block-editor": "^6.1.10",
		"@wordpress/blocks": "^9.1.5",
		"@wordpress/components": "^14.1.7",
		"@wordpress/compose": "^4.1.3",
		"@wordpress/data": "^5.1.3",
		"@wordpress/element": "^3.1.1",
		"@wordpress/hooks": "^3.1.1",
		"@wordpress/i18n": "^4.1.1"
	},
	"devDependencies": {
		"@sixach/wp-block-utils": "^1.0.0",
		"@wordpress/scripts": "^16.1.4",
		"autoprefixer": "^10.3.1",
		"classnames": "^2.3.1",
		"husky": "^7.0.1",
		"lint-staged": "^11.0.1",
		"lodash": "^4.17.21",
		"postcss": "8.3.5",
		"postcss-calc": "^8.0.0",
		"postcss-each": "^1.1.0",
		"postcss-flexbox": "^1.0.3",
		"postcss-for": "^2.1.1",
		"postcss-import": "^14.0.2",
		"postcss-important-startstop": "^2.0.0",
		"postcss-nested": "^4.2.3",
		"postcss-nested-ancestors": "^2.0.0",
		"postcss-position": "^1.1.0",
		"postcss-selector-replace": "^1.0.2",
		"postcss-size": "^4.0.1"
	}
}
